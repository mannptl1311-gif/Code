#include<stdio.h>
int main()
{
float g;
printf("Enter Gross Salary in Rs : ");
scanf("%f",&g);
if (g>10000)
printf("Net salary = %f Rs + (0.1 * %f) Rs - (0.03* %f) Rs = %f Rs", g,g,g,g+(g*0.1)-(g*0.03));
else if (5000<g && g<=10000)
 printf("Net salary = %f Rs + (0.07 * %f) Rs - (0.02* %f) Rs = %f Rs", g,g,g,g+(g*0.07)-(g*0.02));
else
    printf("Can't run \nGross salary should be more than 5000");
return 0;
}